package work;

import java.util.Scanner;

public class work {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("정수 : ");
		String st;
		
		try {
			st = sc.next();
			int num = Integer.parseInt(st);
			System.out.println("결괴 : " + num);
		} catch (Exception e) {
			System.out.println("결과 : " + st + "은 정수가 아닙니다.");
		}
	}
}
